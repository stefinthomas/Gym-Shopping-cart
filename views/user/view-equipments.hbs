                            src="/product-images/{{this._id}}.jpg"
                            alt="Card image cap"
                            style="height: 8rem">
                        <div class="card-body">
                            <h5 class="card-title">{{this.Name}}</h5>
                            <p class="card-text" style="font-weight: bold; ">Rs.{{this.Price}}</p>
                            <p class="card-text">{{this.Description}}</p>
                            
                            <button onclick="addToCart('{{this._id}}')" class="btn btn-info "  >Add to cart</button>
                        </div>
                    </div>
                </div>

