<section>
    <div class="container">
        {{!-- <div class="row mt-4">
            <a href="/admin/add-product" class="btn btn-success ml-auto">Add Product</a>
        </div> --}}
        <table class="table table-dark mt-4">
  <thead class="thead" style="background-color: #1e1f1e; color:white">
    <tr>
      <th>UserId</th>
      <th scope="col">Date</th>
      <th >Address</th>
      <th >Pincode</th>
      <th >Mobile</th>
      <th scope="col">Amount</th>
      <th scope="col">Payment</th>
      <th>Status</th>
      {{!-- <th>Action</th> --}}
      
    </tr>
  </thead>
  <tbody>
      {{#each order}}
    <tr>
      <td>{{this.userId}}</td>
      <td>{{this.date}}</td>
      <td>{{this.deliveryDetails.address}}</td>
      <td>{{this.deliveryDetails.pincode}}</td>
      <td>{{this.deliveryDetails.mobile}}</td>
      <td>{{this.totalAmount}}</td>
      <td>{{this.paymentMethod}}</td>
      <td>{{this.status}}</td>
      <td>
      
        {{!-- <a href="/admin/delete-order/{{this._id}}" class="btn btn-danger" onclick="return confirm('Are you want to delete {{this.userId}} ?')" >Delete</a> --}}
       
      </td>
    </tr>
    {{/each}}
  </tbody>
</table>

    </div>
</section>