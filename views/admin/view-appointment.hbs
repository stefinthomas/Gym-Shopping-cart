<section>
    <div class="container">
        {{!-- <div class="row mt-4 mb-4">
            <a href="/admin/add-product" style="width: 150px; height: 40px" class="btn btn-success ml-auto ">Add Product</a>
        </div> --}}
        <table class="table table-dark mt-5" id="productstable">
  <thead class="thead" style="background-color: #1e1f1e; color:white">
    <tr>
      
      <th scope="col">Membership Id</th>
      <th scope="col">Name</th>
      <th scope="col">Mobile</th>
      <th scope="col">Date</th>
      <th scope="col">Time</th>
    

    </tr>
  </thead>
  <tbody>
      {{#each appointment}}
    <tr>
      
      <td>{{this.MembershipId}}</td>
      <td>{{this.Name}}</td>
      <td>{{this.Mobile}}</td>
      <td>{{this.Date}}</td>
      <td>{{this.Time}}</td>
      {{!-- <td><img style="width: 50px;height:50px;" src="/product-images/{{this._id}}.jpg" alt=""></td> --}}
      <td style='text-align:center;vertical-align:middle'>
        {{!-- <a href="/admin/edit-product/{{this._id}}" class="btn btn-primary">Edit</a> --}}
        <a href="/admin/delete-appointment/{{this._id}}" class="btn btn-danger" onclick="return confirm('Are you want to delete {{this.Name}} ?')" >Delete</a>
      </td>
    </tr>
    {{/each}}
  </tbody>
</table>

    </div>
</section>